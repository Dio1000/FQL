schema: School
using: School

relation: Student
relation: Teacher

Student -> {
    CNP,UUID,PK
    Name,varchar(100),NOT NULL
    Surname,varchar(100),NOT NULL
    isRegistered,boolean,NULLABLE
}

Teacher -> {
    CNP,UUID,PK
    StudentCNP,varchar(20),FK
    Name,varchar(100),NULLABLE
    Surname,varchar(100),NULLABLE
}